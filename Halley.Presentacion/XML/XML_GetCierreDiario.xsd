<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="XML_GetCierreDiario" targetNamespace="http://tempuri.org/XML_GetCierreDiario.xsd" elementFormDefault="qualified" xmlns="http://tempuri.org/XML_GetCierreDiario.xsd" xmlns:mstns="http://tempuri.org/GetCierreDiario.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
